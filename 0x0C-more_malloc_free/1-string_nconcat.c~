#include "holberton.h"
#include <stdlib.h>
#include <stdio.h>
/**
 * count - gets string length
 * @s: input string
 * Return: length
 */
int count(char *s)
{
	int count = 0;

	while (*s)
	{
		count++;
		s++;
	}
	return (count);
}
